<Application x:Class="SharkSpirit.Editor.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:editor="clr-namespace:SharkSpirit.Editor">
    <Application.Resources>
        <ResourceDictionary>
            <editor:ViewModelLocator x:Key="ViewModelLocator"/>

            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="pack://application:,,,/Resources/Brushes/Brushes.xaml" />
                <ResourceDictionary Source="pack://application:,,,/Resources/Styles/ButtonStyles.xaml" />
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>
</Application>
